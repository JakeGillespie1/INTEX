<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Intex Data</title>
    </head>
    <body>
        <table
            style="
                width: 100%;
                background-color: antiquewhite;
                border: 1px solid black;
            "
        >
            <thead style="background-color: cornflowerblue; color: white">
                <tr>
                    <th style="text-align: left">Date</th>
                    <th style="text-align: left">Age</th>
                </tr>
            </thead>
            <tbody style="font-family: Arial; font-size: 9pt">
                <% for (let iCount = 0; iCount < mytest.length; iCount++) { %>
                <tr>
                    <%= let date = new Date(mytest[iCount].date_stamp)%>
                    <td><%= date.toDateString() %></td>
                    <td><%= mytest[iCount].age %></td>
                    <td style="text-align: center">
                        <form
                            action="/editCountry/<%= mytest[iCount].country_id %>"
                            method="GET"
                        >
                            <button type="submit">Edit</button>
                        </form>
                    </td>
                    <td style="text-align: center">
                        <form
                            action="/deleteCountry/<%= mytest[iCount].country_id %>"
                            method="POST"
                        >
                            <button type="submit">Delete</button>
                        </form>
                    </td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </body>
</html>
